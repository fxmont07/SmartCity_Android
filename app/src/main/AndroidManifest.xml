<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.smartcity_android">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.controller.EvaluationListingActivity" />
        <activity android:name=".ui.controller.EvaluateActivity" />
        <activity android:name=".ui.controller.MenuActivity" />
        <activity android:name=".ui.controller.OfferDetailsActivity" />
        <activity android:name=".ui.controller.OfferActivity" />
        <activity android:name=".ui.controller.CriterionStudentActivity" />
        <activity android:name=".ui.controller.ProfilActivity" />
        <activity android:name=".ui.controller.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.controller.SignUpActivity" />
    </application>
    <uses-permission android:name="android.permission.INTERNET"/>
</manifest>